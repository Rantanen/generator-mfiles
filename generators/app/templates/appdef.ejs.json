{
    "guid": "<%= guid %>",
    "name": "<%= projectName %>",
    "description": "<%= projectDescription %>",
    "environments": {
<% for( var i = 0; i < environments.length; i++ ) { -%>
        "<%= environments[ i ] %>": {
            "files": [ "lib/<%= environments[ i ] %>.js" ]
        }<%= i !== environments.length - 1 ? ',' : '' %>
<% } -%>
    }
}
